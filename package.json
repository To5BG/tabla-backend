{
  "name": "tabla-backend",
  "version": "0.0.0",
  "description": "Backend of Tabla webapp",
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "db:start": "bash start-db.sh",
    "db:stop": "docker stop tabla_database_server && docker stop tabla_cache_server",
    "db:flush": "docker volume rm tabla_data && docker volume rm tabla_cache",
    "db:migration:pre": "ts-node -r tsconfig-paths/register prep-type-orm-config.ts",
    "db:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate migration/init -d migration.config.ts",
    "db:migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d migration.config.ts",
    "build": "nest build",
    "prettier": "npx prettier --write --config .prettierrc.json --ignore-path .prettierignore .",
    "eslint": "npx eslint -c .eslintrc.json --ignore-path .eslintignore .",
    "test": "jest --testPathIgnorePatterns=./test/e2e",
    "test:watch": "jest --testPathIgnorePatterns=./test/e2e --watch",
    "test:cov": "jest --testPathIgnorePatterns=./test/e2e --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --testPathPattern=./test/e2e",
    "test:e2e:cov": "jest --testPathPattern=./test/e2e --coverage",
    "jsdoc": "jsdoc -c jsdocconfig.json"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.7.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "better-docs": "^2.7.2",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
